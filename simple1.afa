START:
_1_WHILE:
	SENSE HERE MARKER 0 OR GOTO _1_WHILE_1_NOTEND
	SENSE HERE MARKER 1 OR GOTO _1_WHILE_1_NOTEND
	GOTO _1_WHILE_OR2
_1_WHILE_1_NOTEND:
	GOTO _1_WHILE_OREND
_1_WHILE_OR2:
	SENSE HERE MARKER 2 OR GOTO _1_WHILE_2_OR2
	GOTO _1_WHILE_2_OREND
_1_WHILE_2_OR2:
	SENSE HERE MARKER 3 OR GOTO _1_WHILE_2_NOTEND
_1_WHILE_2_OREND:
	GOTO _1_IFNOT
_1_WHILE_2_NOTEND:
_1_WHILE_OREND:
	MOVE OR JUMP 1
	GOTO _1_WHILE
_1_IFNOT:
	GOTO START